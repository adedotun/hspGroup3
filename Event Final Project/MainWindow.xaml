<Window x:Class="Event_Final_Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Event_Final_Project"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="800">
    <Grid>
            <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top" Background="#FFC4E6F0">
                <Button Content="REGISTER" MinWidth="100" Click="BtnClickRegister" Foreground="White" Background="#FF0F0B0B" />
                <Button Content="PRODUCTS" MinWidth="100" Click="BtnClickProducts" Background="Black" Foreground="White" />
                <Button Content="FEEDBACK" MinWidth="100" Click="BtnClickFeedback" Background="Black" Foreground="White" />
                <Button Content="CONTACT" MinWidth="100" Click="BtnClickContact" Background="Black" Foreground="White" />
                <Button Content="ABOUT" MinWidth="100" Click="BtnClickAbout" Background="#FF050000" Foreground="White" />
            </StackPanel>
            <Frame x:Name="Main" Margin="0,35,0,0" Background="#FFC4E6F0" />
    </Grid>

        
</Window>
